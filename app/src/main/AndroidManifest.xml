<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="yalantis.com.sidemenu.sample" >

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:name="yalantis.com.sidemenu.foodies.app.MyApplication"
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="yalantis.com.sidemenu.foodies.activity.MainActivity"
            android:configChanges="orientation|screenSize"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="yalantis.com.sidemenu.foodies.activity.MenuListActivity"
            android:label="@string/title_activity_menu_list" >
        </activity>
        <activity
            android:name="yalantis.com.sidemenu.foodies.activity.MenuItemListActivity"
            android:label="@string/title_activity_menu_item_list" >
        </activity>
        <activity
            android:name="yalantis.com.sidemenu.foodies.activity.MenuItemDetailActivity"
            android:label="@string/title_activity_menu_item_detail" >
        </activity>
        <activity
            android:name="yalantis.com.sidemenu.foodies.activity.CartActivity"
            android:label="@string/title_activity_cart" >
        </activity>
        <activity
            android:name="yalantis.com.sidemenu.foodies.activity.LoginActivity"
            android:label="@string/title_activity_login" >
        </activity>
    </application>

</manifest>
